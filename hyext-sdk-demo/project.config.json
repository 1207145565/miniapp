{
  "name": "hyext-sdk-demo",
  "outputPath": "dist",
  "releasePath": "release",
  "ignore": [
    "*.md",
    "LICENSE",
    ".git/**",
    ".gitignore",
    "package-lock.json"
  ],
  "builder": {
    "name": "@hyext/builder-beyond",
    "config": {
      "useCssLoader": true,
      "webSDKVersion": "test",
      "buildConfig": {
        "H5": [{
          "entry": "index.js",
          "extType": "web_video_com",
          "platform": "web"
        }, {
          "entry": "index_streamer.js",
          "extType": "pc_anchor_panel",
          "platform": "pc"
        }, {
          "entry": "index.js",
          "extType": "web_popup",
          "platform": "web"
        }, {
          "entry": "index.js",
          "extType": "pc_panel",
          "platform": "pc"
        }, {
          "entry": "index.js",
          "extType": "pc_popup",
          "platform": "pc"
        }],
        "RN": [{
          "entry": "index.js",
          "extType": "app_panel",
          "platform": "app"
        }, {
          "entry": "index_streamer.js",
          "extType": "zs_anchor_panel",
          "platform": "app"
        }]
      }
    }
  }
}
